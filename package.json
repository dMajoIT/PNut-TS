{
  "name": "pnuts",
  "version": "0.0.1",
  "description": "A Parallax Propeller compiler in Typecsript",
  "main": "dist/pnuts.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && dist/pnuts.js",
    "test": "jest",
    "watch": "tsc -b -w",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ironsheep/P2-vscode-langserv-extension"
  },
  "keywords": [
    "cli",
    "typescript",
    "propeller2",
    "parallax.com"
  ],
  "author": "Stephen M Moraco",
  "publisher": "IronSheepProductionsLLC",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^26.6.3",
    "jest-mock-console": "^2.0.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "commander": "^7.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
