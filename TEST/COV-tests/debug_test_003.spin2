_clkfreq = 10_000_000

dat	org

loop	debug(uhex_long(i))
	add	i,#1
  debug(` `$(i) `#(i))
  debug(` `$(i))
  debug(if(i), udec(i), dly(#250)) ' <-- FAILS
  'debug(ifnot(i), udec(i), dly(#250)) ' <-- FAILS
  debug(` `if(i), `(i), `dly(#250))
  debug(` `ifnot(i), `(i), `dly(#250))
	jmp	#loop

i	res	1
'{
PUB main() | var1, var2

    debug
    debug()

    debug(if(var1 & 1), udec(var1), dly(250))
    debug(ifnot(var1 & $02), udec_(var2), dly(250))
    debug(` `if(var1 & 1), `$(var1), `dly(250)) ' <-- FAILS
    debug(` `ifnot(var1 & $02), `$(var2), `dly(250)) ' <-- FAILS
    debug(` `ifnot(0) `$(var1)  `%(var2))
'}
