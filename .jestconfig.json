{
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 100,
      "lines": 90
    }
  },
  "coverageReporters": ["lcov"],
  "coverageDirectory": "./output/code-coverage",
  "coveragePathIgnorePatterns": ["/node_modules/", "/src/tests/"],
  "collectCoverageFrom": ["dist/**/*.{js,jsx,ts,tsx}", "!dist/tests"]
}
