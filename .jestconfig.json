{
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 100,
      "lines": 90
    }
  },
  "coverageReporters": ["lcov"],
  "coverageDirectory": "out-jest/code-coverage",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/dist/tests/",
    "/dist/ext/",
    "/src/tests/",
    "/src/ext/",
    ".test.js",
    "testUtils.js"
  ],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "dist/**/*.js",
    "src/**/*.ts",
    "!dist/**/*.{test,testUtils}.{js,jsx}",
    "!src/**/*.{test,testUtils}.{ts,tsx}"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "verbose": true
}
